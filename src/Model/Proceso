public class Process {
    private int id;
    private int arrivalTime;
    private int priority;
    private int cpuTimeRequired;
    private int cpuTimeRemaining;
    private int memoryRequired;
    private int printers;
    private int scanners;
    private int webcams;
    private int speakers;

    // Constructor
    public Process(int id, int arrivalTime, int priority, int cpuTimeRequired,
                   int memoryRequired, int printers, int scanners, int webcams, int speakers) {
        this.id = id;
        this.arrivalTime = arrivalTime;
        this.priority = priority;
        this.cpuTimeRequired = cpuTimeRequired;
        this.cpuTimeRemaining = cpuTimeRequired;
        this.memoryRequired = memoryRequired;
        this.printers = printers;
        this.scanners = scanners;
        this.webcams = webcams;
        this.speakers = speakers;
    }

    

    // Getters y setters para cada atributo
    // MÃ©todos adicionales para reducir cpuTimeRemaining, cambiar prioridad, etc.
}
